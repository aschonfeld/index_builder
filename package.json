{
  "name": "index_builder",
  "version": "0.2.0",
  "description": "Index Builder",
  "main": "main.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test-file": "tape -r babel-register -r ./test_env \"$TEST\" | tap-diff",
    "test-all": "tape -r babel-register -r ./test_env 'static/__tests__/{,**/}*-test.{js,jsx}'",
    "test": "npm run test-all -s | tap-diff",
    "test-with-coverage": "NODE_ENV=test node --max-old-space-size=8192 --optimize-for-size ./node_modules/nyc/bin/nyc.js --reporter html --reporter lcovonly --all npm run test-all -s | ntee results.tap | tap-diff && less results.tap | tap-xunit --package=grailjs > js_junit.xml && nyc report",
    "test-with-junit": "npm run test-all -s | tap-xunit --package=grailjs > js_junit.xml",
    "report-duplicate-code": "jsinspect -t 20 static --reporter pmd > duplicates.xml; echo 'Wrote duplicates.xml'",
    "watch-js": "webpack --watch --progress",
    "watch": "npm run watch-js",
    "build": "webpack --config ./webpack.production.config.js",
    "format": "npm run sort-imports && npm run format-prettier",
    "format-eslint": "eslint static js *.js .*.js --ext .js --ext .jsx -f node_modules/eslint-clang-formatter -c .eslint.format.json --no-eslintrc --fix --ignore-pattern !.eslintrc.json >/dev/null || true ",
    "format-prettier": "prettier --write \"static/**/*.{js,jsx,css}\" *.js",
    "sort-imports": "find static -name '*.js' -o -name '*.jsx' -exec js-import-sort --path {} +",
    "lint": "npm run lint-js",
    "lint-js": "eslint static *.js --ext .js --ext .jsx -f node_modules/eslint-clang-formatter",
    "lint-js-file": "eslint -f node_modules/eslint-clang-formatter",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "report-coverage": "cat ./JS_coverage/lcov.info | coveralls"
  },
  "nyc": {
    "include": [
      "static"
    ],
    "exclude": [
      "venv/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**"
    ],
    "extension": [
      ".jsx"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "report-dir": "./JS_coverage"
  },
  "author": "",
  "private": true,
  "license": "SEE LICENSE IN proprietary",
  "devDependencies": {
    "autoprefixer": "8.3.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "coveralls": "3.0.0",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-clang-formatter": "1.3.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-lodash": "2.7.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-tape": "1.1.0",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "imports-loader": "0.8.0",
    "js-import-sort": "1.2.0",
    "jsdom": "11.8.0",
    "jsdom-global": "3.0.2",
    "jsinspect": "0.12.7",
    "node-hook": "1.0.0",
    "nyc": "11.7.1",
    "postcss-loader": "2.1.4",
    "postcss-nested": "3.0.0",
    "prettier": "1.12.1",
    "proxyquire": "2.0.0",
    "react-test-renderer": "16.3.2",
    "sinon": "4.5.0",
    "style-loader": "0.21.0",
    "tap-diff": "0.1.1",
    "tap-xunit": "2.3.0",
    "tape": "4.9.0",
    "tape-catch": "1.0.6",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14"
  },
  "dependencies": {
    "any-promise": "1.3.0",
    "chart.js": "2.7.2",
    "create-react-class": "15.6.3",
    "es6-object-assign": "1.1.0",
    "es6-promise": "4.2.4",
    "expose-loader": "0.7.5",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "json-loader": "0.5.7",
    "lodash": "4.17.5",
    "moment": "2.22.1",
    "ntee": "1.1.5",
    "popsicle": "9.2.0",
    "postcss-cli": "5.0.0",
    "prop-types": "15.6.1",
    "querystring": "0.2.0",
    "react": "16.3.2",
    "react-addons-shallow-compare": "15.6.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0",
    "string.prototype.startswith": "0.2.0",
    "tap-nyc": "1.0.3",
    "uuid": "3.2.1"
  }
}
